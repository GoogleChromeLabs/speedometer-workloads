{
    "name": "todomvc-es6-webpack",
    "version": "1.0.0",
    "author": "Thorsten Kober",
    "license": "SEE LICENSE IN LICENSE",
    "engines": {
        "node": ">=18.13.0",
        "npm": ">=8.19.3"
    },
    "private": true,
    "scripts": {
        "prepare": "HOST='./dist' node node_modules/benchmark-connector/dist/prepare.min.js",
        "dev": "npm run prepare && webpack serve --config webpack.dev.js",
        "build:static": "webpack --config webpack.prod.js && npm run prepare",
        "start:static": "http-server ./dist -c-1 --cors --silent"
    },
    "dependencies": {
        "http-server": "^14.1.1",
        "todomvc-app-css": "^2.4.2"
    },
    "devDependencies": {
        "benchmark-connector": "workspace: *",
        "copy-webpack-plugin": "^11.0.0",
        "css-loader": "^6.7.3",
        "css-minimizer-webpack-plugin": "^4.2.2",
        "html-webpack-plugin": "^5.5.0",
        "mini-css-extract-plugin": "^2.7.2",
        "style-loader": "^3.3.1",
        "webpack": "^5.75.0",
        "webpack-cli": "^5.0.1",
        "webpack-dev-server": "^4.11.1",
        "webpack-merge": "^5.8.0"
    }
}
