{
    "name": "todomvc-preact",
    "version": "1.0.0",
    "author": "Thorsten Kober",
    "license": "SEE LICENSE IN LICENSE",
    "engines": {
        "node": ">=18.13.0",
        "npm": ">=8.19.3"
    },
    "private": true,
    "scripts": {
        "prepare": "HOST='./public' node node_modules/benchmark-connector/dist/prepare.min.js",
        "dev": "npm run prepare && npm-run-all --parallel start:static watch",
        "build:static": "npm run prepare && rollup -c --bundleConfigAsCjs",
        "start:static": "http-server ./dist -c-1 --cors --silent",
        "watch": "ROLLUP_WATCH=true rollup -c -w --bundleConfigAsCjs"
    },
    "devDependencies": {
        "@babel/plugin-transform-react-jsx": "^7.21.0",
        "@babel/preset-env": "^7.20.2",
        "@babel/preset-react": "^7.18.6",
        "@rollup/plugin-babel": "^6.0.3",
        "@rollup/plugin-commonjs": "^24.0.1",
        "@rollup/plugin-node-resolve": "^15.0.1",
        "@rollup/plugin-terser": "^0.4.0",
        "eslint-plugin-react": "^7.32.2",
        "npm-run-all": "^4.1.5",
        "rollup": "^3.19.1",
        "rollup-plugin-copy": "^3.4.0",
        "rollup-plugin-gzip": "^3.1.0",
        "rollup-plugin-import-css": "^3.2.1",
        "rollup-plugin-postcss": "^4.0.2",
        "rollup-plugin-resolve": "^0.0.1-predev.1"
    },
    "dependencies": {
        "benchmark-connector": "workspace: *",
        "classnames": "^2.3.2",
        "http-server": "^14.1.1",
        "linkstate": "^2.0.1",
        "preact": "^10.13.1",
        "preact-router": "^4.1.0",
        "todomvc-app-css": "^2.4.2"
    }
}
