{
    "name": "perf.webkit.org",
    "version": "1.0.0",
    "engines": {
        "node": ">=18.13.0",
        "npm": ">=8.19.3"
    },
    "private": true,
    "scripts": {
        "prepare": "HOST='./public/v3' node node_modules/benchmark-connector/dist/prepare.min.js",
